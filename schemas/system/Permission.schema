System({
  Role: { category: 'Role', required: true, index: true },
  Category: { category: 'Category', required: true },
  Catalog: { category: 'Catalog' },
  Division: { category: 'Division' },
  Access: { domain: 'UserAccess', required: true },

  NaturalKey: Index('Role', 'Category'),

  CatalogOrDivision: Validate(record => {
    const notDivision = !record.Division;
    record.Catalog ? notDivision : !notDivision;
  })
})
