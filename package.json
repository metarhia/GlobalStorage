{
  "name": "globalstorage",
  "version": "0.3.0",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Global Storage is a Global Distributed Data Warehouse",
  "license": "MIT",
  "keywords": [
    "database",
    "storage",
    "global",
    "warehouse",
    "metarhia",
    "impress",
    "distributed",
    "dbms"
  ],
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/globalstorage"
  },
  "main": "gs.js",
  "browser": {
    "gs.js": "dist/gs.js",
    "lib/fs.cursor.js": false,
    "lib/fs.provider.js": false
  },
  "scripts": {
    "test": "npm run lint && metatests test/",
    "lint": "eslint . --ignore-path .gitignore",
    "build": "babel gs.js -d dist && babel lib -d dist/lib --ignore lib/fs.provider.js,lib/fs.cursor.js",
    "prepublish": "npm run -s build"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@metarhia/jstp": "^2.1.0",
    "mdsf": "^1.0.2",
    "metarhia-common": "^0.0.32",
    "metaschema": "0.0.27",
    "metasync": "^0.3.30",
    "pg": "^7.6.1"
  },
  "optionalDependencies": {
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "eslint": "^5.9.0",
    "eslint-config-metarhia": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "metatests": "^0.2.4"
  }
}
